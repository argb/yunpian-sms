{
    "name": "ender/yunpian-sms",
    "description": "A php SDK for yunpian(云片网络短信服务商) sms RESTFULL API",
    "keywords": [
        "sms",
        "php",
        "duanxin"
    ],
    "homepage": "https://www.bigfeettrip.com",
    "license": "MIT",
    "authors": [
        {
            "name": "ender",
            "email": "511715505@qq.com",
            "homepage": "https://www.bigfeettrip.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Ender\\YunPianSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ender\\YunPianSms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
